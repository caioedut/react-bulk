    "prebuild": "npm run clean",
    "build": "lerna exec --parallel -- rollup -c ../../rollup.config.js",
    "publish:stable": "npm run build && lerna publish"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "^18.7.6",
