  "license": "MIT",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "example:web": "yarn build && yarn --cwd ./examples/web dev",
    "example:native": "yarn build && yarn --cwd ./examples/native dev",
    "bootstrap": "lerna bootstrap",
