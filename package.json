    "bili": "^5.0.5",
    "lerna": "^5.1.4",
    "prettier": "^2.6.2",
    "react": "^17.0.2",
    "react-native": "^0.63.4",
    "react-native-svg": "^13.2.0",
    "rimraf": "^3.0.0",
    "rollup": "2.38.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.29.0",
    "rollup-plugin-url": "3.0.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {}
