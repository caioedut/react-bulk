{
  "name": "react-bulk",
  "version": "0.0.2",
  "private": true,
  "license": "MIT",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "deploy": "yarn deploy:core && yarn deploy:web && yarn deploy:native",
    "deploy:core": "yarn workspace @react-bulk/core publish --access public --scope=@caioedut",
    "deploy:web": "yarn workspace @react-bulk/web publish --access public",
    "deploy:native": "yarn workspace @react-bulk/native publish --access public"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "prettier": "^2.6.2"
  }
}
